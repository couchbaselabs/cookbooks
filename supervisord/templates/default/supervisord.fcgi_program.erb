[fcgi-program:<%= @name %>]
socket=unix://<%= @socket %>
socket_mode=<%= @socket_mode %>
socket_owner=<%= @owner %>
command=<%= @command %>
environment=<%= @environment %>
user=<%= @user %>
numprocs=<%= @process_number %>
autorestart=true
autostart=true
startsecs=0
startretries=<%= @retries %>
stopsignal=QUIT
stopwaitsecs=1
redirect_stderr=true
stdout_logfile=<%= node['supervisord']['childlogdir'] %><%= @name %>.log
stdout_logfile_maxbytes=10MB
directory=/tmp
process_name=%(program_name)s_%(process_num)02d
